<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_and_End_Time_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Start_Time__c &gt; End_Time__c, Start_Time__c = End_Time__c)</errorConditionFormula>
    <errorMessage>Start Time should only be less than End Time,</errorMessage>
</ValidationRule>
